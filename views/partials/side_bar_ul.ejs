
 <div class="side-bar-header-conatiner">
	 
	<%if (role === 'Parent') { %>
	<div class="main-selected-student">
		<img class="side-bar-brand" src="<%= student.Student_Image %>" alt="brand"/>
		<span class="side-bar-brand-label"><%= student.Student_FirstName %> <%= student.Student_LastName %></span>
		<%if (students.length > 1) { %>
			<img class="icon button-icon" src="assets/icons/caret_brand.svg">
		<% } %>
	</div>
	<%if(students.length && role === 'Parent') {%>
		<div class="student-dropdown-list slide-up">
			<% for(var i=0; i <= students.length - 1; i++) {%>
