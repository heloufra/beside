                                            <th scope="col" data-lang="Type">Type</th>
                                            <th scope="col" data-lang="From">From</th>
                                            <th scope="col" data-lang="To">To</th>
                                          </tr>
                                        </thead>

                                        <tbody class="table_reported">

                                        
                                        </tbody>

                                      </table>
                                    </div>

                                  </div>                                            

                                  <div id="absence_title" class="sections-main-sub-container-right-main-label-divider hidden">
