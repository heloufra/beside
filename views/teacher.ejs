                                  <div class="sections-main-sub-container-right-main-rows sections-main-sub-container-right-main-rows-dropdown-tags subjects-list">

                                      

                                  </div>

                                  <!-- End Subject Class --------------------------------------------------------------------------------->

                                  <!--  sub-container-form-footer -->
                                
                                  <!--  End sub-container-form-footer -->

                              </div>
                              <!-- End sections-main-sub-container-right-main-body : Details -->


                              <!-- sections-main-sub-container-right-main-body : Absence -->
                              <div id="Absence" class="tab-pane fade  sections-main-sub-container-right-main-body">

                                  <div id="reported_title" class="sections-main-sub-container-right-main-label-divider hidden">
                                        <p data-lang="Reported_absence">Reported absence</p>
